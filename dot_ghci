:def k! \x   -> return $ ":kind! " ++ x
:def pf   \str -> return $ ":! pointfree \'" ++ str ++ "\'"
:def unpf \str -> return $ ":! pointful  \'" ++ str ++ "\'"

:set -XOverloadedStrings
:set -XTupleSections
:set prompt "Î»> "
:set prompt2 ":{ "

